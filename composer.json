{
    "name": "dantleech/phpactor",
    "minimum-stability": "dev",
    "require": {
        "symfony/console": "~2.7",
        "roave/better-reflection": "dev-variable_collector",
        "phpbench/container": "^1.0",
        "symfony/finder": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/dantleech/BetterReflection" }
    ],
    "autoload": {
        "psr-4": {
            "Phpactor\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Tests\\": "tests/",
            "Phpactor\\Benchmark\\": "benchmark/"
        }
    }
}
