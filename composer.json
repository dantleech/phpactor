{
    "name": "phpactor/phpactor",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "phpbench/container": "^1.0",
        "phpactor/class-mover": "dev-master",
        "phpactor/source-code-filesystem": "dev-master",
        "phpactor/class-to-file": "dev-master",
        "phpactor/code-transform": "dev-master",
        "phpactor/worse-reflection": "dev-master",
        "phpactor/path-finder": "dev-master",
        "symfony/filesystem": "^3.3",
        "webmozart/glob": "^4.1",
        "dnoegel/php-xdg-base-dir": "^0.1.0",
        "symfony/yaml": "^3.3",
        "monolog/monolog": "^1.0",
        "phpactor/completion": "^1.0@dev",
        "composer/xdebug-handler": "^1.1",
        "sebastian/diff": "2.0.x-dev",
        "ocramius/package-versions": "^1.2",
        "phpactor/language-server": "dev-master",
        "phpactor/container": "^1.0",
        "phpactor/class-to-file-extension": "dev-master",
        "phpactor/completion-extension": "dev-master",
        "phpactor/completion-rpc-extension": "dev-master",
        "phpactor/completion-worse-extension": "dev-master",
        "phpactor/composer-autoloader-extension": "dev-master",
        "phpactor/file-path-resolver-extension": "dev-master",
        "phpactor/logging-extension": "^0.1.0",
        "phpactor/rpc-extension": "dev-master",
        "phpactor/source-code-filesystem-extension": "dev-master",
        "phpactor/worse-reflection-extension": "dev-master",
        "phpactor/console-extension": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/process": "^2.7|^3.0",
        "symfony/options-resolver": "^2.7|^3.0",
        "phpbench/phpbench": "^1.0@dev",
        "phpactor/test-utils": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.12@dev"
    },
    "bin": [
        "bin/phpactor"
    ],
    "autoload": {
        "psr-4": {
            "Phpactor\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        }
    }
}
