Before:
  Save g:projectDirs
  Save g:phpactorProjectRootPatterns
  Save g:phpactorGlobalRootPatterns
  Save g:phpactorInitialCwd

  let fixtureDir = tempname()
  let g:projectDirs = {
    \'SillyWalksProject': fixtureDir.'/SillyWalksProject',
    \'TwoShedsProject': g:fixtureDir.'/TwoShedsProject'
    \ }

  call mkdir(g:projectDirs['SillyWalksProject'].'/subdir', 'p')
  call mkdir(g:projectDirs['TwoShedsProject'].'/subdir', 'p')

  let g:phpactorGlobalRootPatterns = ['/', '/home']
  let g:phpactorProjectRootPatterns = ['composer.json', '.git', '.phpactor.json', '.phpactor.yml']
  let g:phpactorInitialCwd = '/'
  let g:phpactorProjectRepository = phpactor#project#repository#create()
  let g:phpactorProjectAssigner = phpactor#project#assigner#create(
      \ g:phpactorProjectRepository,
      \ g:phpactorProjectRootPatterns,
      \ g:phpactorGlobalRootPatterns,
      \ g:phpactorInitialCwd
      \ )

After:
  Restore
